{
  "format": "page",
  "id": "homepage",
  "label": "Homepage",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "heading1,strong"
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": ""
        }
      },
      "articledetail": {
        "type": "Link",
        "config": {
          "label": "articleDetail",
          "select": "document",
          "repeat": false,
          "customtypes": ["article"]
        }
      },
      "articleitem": {
        "type": "Group",
        "config": {
          "label": "articleItem",
          "repeat": true,
          "fields": {
            "articles": {
              "type": "Link",
              "config": {
                "label": "articles",
                "select": "document",
                "repeat": false,
                "customtypes": ["article"]
              }
            }
          }
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "article_reference": {
              "type": "SharedSlice"
            },
            "two_column_poem_layout": {
              "type": "SharedSlice"
            },
            "footer_simple_grid": {
              "type": "SharedSlice"
            },
            "hero_with_split_columns": {
              "type": "SharedSlice"
            },
            "navigation_bar": {
              "type": "SharedSlice"
            },
            "hero": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
